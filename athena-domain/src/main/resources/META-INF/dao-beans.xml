<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="classpath:META-INF/datasource-beans.xml"/>

    <bean id="domainDAO" class="org.odhsi.athena.dao.impl.DomainDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="attributeDefinitionDAO" class="org.odhsi.athena.dao.impl.AttributeDefinitionDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="cohortAttributeDAO" class="org.odhsi.athena.dao.impl.CohortAttributeDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="cohortDefinitionDAO" class="org.odhsi.athena.dao.impl.CohortDefinitionDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="conceptDAO" class="org.odhsi.athena.dao.impl.ConceptDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="conceptAncestorDAO" class="org.odhsi.athena.dao.impl.ConceptAncestorDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="conceptRelationshipDAO" class="org.odhsi.athena.dao.impl.ConceptRelationshipDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="drugStrengthDAO" class="org.odhsi.athena.dao.impl.DrugStrengthDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="sourceToConceptMap" class="org.odhsi.athena.dao.impl.SourceToConceptMapDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="conceptClassDAO" class="org.odhsi.athena.dao.impl.ConceptClassDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="conceptSynonymDAO" class="org.odhsi.athena.dao.impl.ConceptSynonymDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="relationshipDAO" class="org.odhsi.athena.dao.impl.RelationshipDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="vocabularyDAO" class="org.odhsi.athena.dao.impl.VocabularyDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>

    <bean id="vocabularyBuildLogDAO" class="org.odhsi.athena.dao.impl.VocabularyBuildLogDAOImpl">
        <property name="dataSource" ref="dataSource"/>
    </bean>
</beans>